{% extends 'base.html.twig' %}

{% block title %}ACTOR List{% endblock %}

{% block body %}
<div class="row">
	<div class="jumbotron text-center">
		<p>Actor Details</p> 
	</div>
	<div class="container">
		<h2>Actor Details</h2>
		<table class="table table-striped">
			<thead>
				<tr>
					<th>Actor ID</th>
					<th>Actor Name</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<td>{{ actor.id }}</td>
					<td>{{ actor.name }}</td>
				</tr>
			</tbody>
	</div>
</div>
{% endblock %}
